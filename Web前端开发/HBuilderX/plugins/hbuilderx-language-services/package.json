{
	"name": "hbuilderx-language-services",
	"displayName": "内置语言服务",
	"description": "内置语言服务，包含js、ts、vue、html、css、less、scss、stylus、json",
	"version": "1.0.0-2022071922",
	"__from": "HBuilderX",
	"__private": true,
	"author": "dcloudio",
	"publisher": "dcloudio",
	"engines": {
		"HBuilderX": "^3.2.12",
		"vscode": "^1.22.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript/undisplay.json"
			}
		],
		"commands": [
			{
				"command": "typescript.openTsServerLog2",
				"title": "查看TsService运行日志",
				"category": "TypeScript"
			},
			{
				"command": "emmet.expandAbbreviation",
				"title": "Expand Emmet",
				"category": "TypeScript"
			},
			{
				"command": "indexService.rebuildIndex",
				"title": "%menu.rebuidindex.name%"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "indexService.rebuildIndex",
					"group": "refresh",
					"when": "explorerResourceIsWorkspaceFolder"
				},
				{
					"group": "z_commands"
				}
			]
		}
	},
	"scripts": {
		"build": "node scripts/build.js",
		"clean": "tsc -b --clean",
		"release": "node scripts/release.js",
		"watch": "node scripts/watch.js",
		"check": "node scripts/check.js"
	},
	"dependencies": {
		"@emmetio/html-matcher": "^0.3.3",
		"@emmetio/math-expression": "^1.0.4",
		"@vscode/emmet-helper": "^2.3.0",
		"chardet": "1.4.0",
		"chokidar": "^3.5.3",
		"css-parser2": "^0.4.0",
		"element-helper-json": "^2.0.6",
		"gridsome-helper-json": "^1.0.3",
		"image-size": "^1.0.1",
		"js-beautify": "^1.14.0",
		"jsonc": "2.0.0",
		"jsonc-parser": "^2.2.1",
		"md5": "2.3.0",
		"rimraf": "^2.6.3",
		"semver": "5.5.1",
		"stylus": "^0.55.0",
		"typescript": "4.5.5",
		"typescript-vscode-sh-plugin": "^0.6.14",
		"vscode-css-languageservice": "^4.3.3",
		"vscode-html-languageservice": "^3.1.2",
		"vscode-json-languageservice": "4.1.0",
		"vscode-languageclient": "7.0.0-next.5.1",
		"vscode-languageserver": "7.0.0-next.3",
		"vscode-languageserver-protocol": "3.16.0-next.4",
		"vscode-languageserver-textdocument": "^1.0.1",
		"vscode-nls": "^4.1.2",
		"vscode-uri": "^3.0.3",
		"vue-onsenui-helper-json": "^1.0.2"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/node": "12.x",
		"@types/rimraf": "2.0.2",
		"@types/semver": "^5.5.0",
		"chalk": "^4.1.2",
		"execa": "^5.1.1",
		"fs-extra": "^10.0.0",
		"glob": "7.2.0",
		"gulp": "^4.0.2",
		"gulp-uglify-es": "^3.0.0",
		"merge-options": "^3.0.4",
		"minimist": "^1.2.5",
		"mocha-junit-reporter": "^1.17.0",
		"mocha-multi-reporters": "^1.1.7"
	}
}
